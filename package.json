{
  "name": "com.gamua.flappy-starling",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@apache-royale/cli": "^0.2.2",
    "@apache-royale/royale-js": "^0.9.11",
    "asconfigc": "^1.8.0",
    "cpy-cli": "^5.0.0",
    "openfl": "^8.9.6",
    "starling-framework": "^2.5.1"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "cpy node_modules/openfl/dist/openfl.min.js bin/js-debug && asconfigc --debug=true",
    "build:prod": "cpy node_modules/openfl/dist/openfl.min.js bin/js-release && asconfigc --debug=false",
    "start": "npm run start:dev",
    "start:dev": "npm run build:dev && royale serve:debug",
    "start:prod": "npm run build:prod && royale serve:release"
  }
}
